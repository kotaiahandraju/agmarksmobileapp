<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/bgn"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"

    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="1dp"
        android:orientation="horizontal" >
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="@dimen/_138sdp"
            android:id="@+id/linear1"
            android:layout_margin="1dp"
            android:background="@drawable/linear"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical" >
            <ImageView
                android:id="@+id/flag1"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_gravity="center"
                android:background="@drawable/login"
                android:backgroundTint="@color/white"
                 />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Login"
                android:layout_marginTop="8dp"
                android:id="@+id/text1"
                android:textSize="13dp"
                android:textStyle="bold"
                android:textColor="@color/white"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="@dimen/_138sdp"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:id="@+id/linear2"
            android:gravity="center"
            android:background="@drawable/linear"
            android:orientation="vertical" >
            <ImageView
                android:id="@+id/flag2"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_gravity="center"
                android:backgroundTint="@color/white"
                android:background="@drawable/registration"

               />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Registration"
                android:layout_marginLeft="5dp"
                android:textSize="13dp"
                android:layout_marginTop="8dp"
                android:id="@+id/text2"
                android:textStyle="bold"
                android:textColor="@color/white"/>
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal" >
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="@dimen/_138sdp"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:id="@+id/linear3"
            android:gravity="center"
            android:background="@drawable/linear"
            android:orientation="vertical" >
            <ImageView
                android:id="@+id/flag3"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_gravity="center"
                android:backgroundTint="@color/white"
                android:background="@drawable/weather"
                 />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Weather"
                android:layout_marginLeft="5dp"
                android:textSize="13dp"
                android:layout_marginTop="8dp"
                android:textStyle="bold"
                android:id="@+id/text3"
                android:textColor="@color/white"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="@dimen/_138sdp"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@drawable/linear"
            android:id="@+id/linear4"
            android:orientation="vertical" >
            <ImageView
                android:id="@+id/flag4"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_gravity="center"
                android:backgroundTint="@color/white"
                android:background="@drawable/marketprice"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="MarketPrices"
                android:textStyle="bold"
                android:layout_marginLeft="5dp"
                android:textSize="13dp"
                android:layout_marginTop="8dp"
                android:id="@+id/text4"
                android:textColor="@color/white"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal" >
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="@dimen/_138sdp"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@drawable/linear"
            android:id="@+id/linear5"
            android:orientation="vertical" >
            <ImageView
                android:id="@+id/flag5"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_gravity="center"
                android:backgroundTint="@color/white"
                android:background="@drawable/newsfeed"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="News Feed"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="5dp"
                android:textSize="13dp"
                android:textStyle="bold"
                android:id="@+id/text5"
                android:textColor="@color/white"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="@dimen/_138sdp"
            android:layout_margin="1dp"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@drawable/linear"
            android:id="@+id/linear6"
            android:orientation="vertical" >
            <ImageView
                android:id="@+id/flag6"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_gravity="center"
                android:backgroundTint="@color/white"
                android:background="@drawable/expertadvice"
                tools:ignore="ContentDescription" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Expert Advice"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="5dp"
                android:textSize="13dp"
                android:textStyle="bold"
                android:id="@+id/text6"
                android:textColor="@color/white"
                android:layout_marginStart="5dp" />
        </LinearLayout>
    </LinearLayout>
    </LinearLayout>
</FrameLayout>
